<div class="container">
  <div class="row row no-gutters">
    <h4>User All</h4>
  </div>
</div>

<% @users.each do |user| %>
<div class="container">
  <div class="row">
    <div class="col-1"></div>
    <div class="col-5">
      <i class="fa-solid fa-user"></i>
      <%= link_to admin_user_path(user.id) do %>
        <%= user.name %>
      <% end %>
    </div>
  </div>
</div>
<% end %>
