<%= form_with url: search_photo_images_path, local: true, method: :get do |f| %>
  <%= f.label :検索 %>
    <%= f.date_select :search, {start_year: 2022, end_year: (Time.now.year - 18), include_blank: true, use_two_digit_numbers: true} %>
    <!--↑セレクトボックスにしてもいい　※ただし、年が4桁、月日が二桁で選択できるようにすること-->
  <%= submit_tag 'Search', class: "btn btn-outline-lightgreen" %>
<% end %>