<%- if controller_name != 'sessions' %>
  <%= link_to "LOGIN", new_session_path(resource_name), class: "btn btn-outline-blue w-25" %><br />
<% end %>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録", new_registration_path(resource_name), class: "btn btn-outline-lightgreen w-25" %><br />
<% end %>