  <h4>User All</h4>

  <% @users.each do |user| %>
    <i class="fa-solid fa-user"></i>
    <%= link_to user_path(user.id) do %>
      <%= user.name %>
    <% end %>

    <i class="fa-solid fa-cake-candles"></i><%= user.birthdate %>
    <i class="fa-solid fa-circle-info"></i><%= user.introduction %>
    <br>
  <% end %>

