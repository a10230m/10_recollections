  <% if photo_image.favorited_by?(current_user) %>
    <td>
      <%= link_to photo_image_favorites_path(photo_image), method: :delete, remote: true do %>
        <i class="fa-solid fa-heart hearton"></i>
      <% end %>
      <%= photo_image.favorites.count %>
    </td>
  <% else %>
    <td>
      <%= link_to photo_image_favorites_path(photo_image), method: :post, remote: true do %>
        <i class="fa-solid fa-heart heartoff"></i>
      <% end %>
      <%= photo_image.favorites.count %>
    </td>
  <% end %>


